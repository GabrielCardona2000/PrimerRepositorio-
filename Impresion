// Clase Impresión
class Impresion {
    private String color;
    private Foto[] fotos;

    public Impresion(String color, Foto[] fotos) {
        this.color = color;
        this.fotos = fotos;
    }

    public void imprimirFotos() {
        System.out.println("Impresión en " + color);
        for (Foto foto : fotos) {
            foto.print();
        }
    }
}
